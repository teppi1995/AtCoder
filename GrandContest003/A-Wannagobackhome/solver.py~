import logging

logging.basicConfig(level=logging.INFO, format="%(message)s")
#logging.disable(logging.CRITICAL)

def main():
    trip  = input()
    logging.info("Hello!")

    if (trip.count("S") == trip.count("N")) and (trip.count("E") == trip.count("W")):
        print("Yes")
    else:
        print("No")
        
if __name__ == "__main__":
    main()
