import logging

logging.basicConfig(level=logging.INFO, format="%(message)s")
#logging.disable(logging.CRITICAL)

def main():
    A_1, A_2, A_3 = map(int, input().split())
    logging.info("Hello!")

    if (A_1 + A_2 + A_3) > 21:
        print("bust")
    else:
        print("win")
        
if __name__ == "__main__":
    main()
